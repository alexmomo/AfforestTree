
<#macro html>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
 <html>
	 <head>
	    <meta http-equiv="Content-Type" content="text/html; charset=${charset}"/>
	    <base href="${basePath}" />
		<title>${title}</title>
		<meta name="keywords" content="${keywords}"/>
		<meta name="description" content="${description}"/>
		<link rel="shortcut icon" href="${domain}/${ico}" type="image/x-icon"/>
		<#list cssList as css>
			<link rel="stylesheet" href="${domain}/css/${css}${version}" type="text/css"/>
		</#list>	
		<#list jsList as js>
		 	<script type="text/javascript" src="${domain}/js/${js}${version}""></script>	 
		</#list>
		<#if checkMobileRedirect>
			<script type="text/javascript">checkuaredirect();</script>
		</#if>	
	 </head>
	 <body>
	 	<div style="width:${pageWidth}px" id="n_wrapper">
	 		${page_header}
	 		${search_box}
	 		<#nested>
	 		${page_bottom}
    	</div> 	    
	 </body>	 
 </html>
 </#macro>